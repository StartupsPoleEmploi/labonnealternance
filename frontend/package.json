{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://127.0.0.1:8000/",
    "engineStrict": true,
    "engines": {
        "node": ">=9.0.0 <12.0.0"
    },
    "dependencies": {
        "@marvelapp/react-ab-test": "2.3.0",
        "@reach/router": "1.3.3",
        "babel-core": "7.0.0-bridge.0",
        "babel-polyfill": "6.26.0",
        "browserstack-local": "1.4.5",
        "create-react-app": "3.4.1",
        "leaflet.markercluster": "1.4.1",
        "mapbox.js": "3.3.1",
        "mixpanel-browser": "2.37.0",
        "nightwatch": "1.3.6",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-ga": "3.0.0",
        "react-redux": "7.2.0",
        "redux": "4.0.5"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "./node_modules/.bin/npm-run-all -p watch-css start-js",
        "build-js": "react-scripts build",
        "build": "./node_modules/.bin/npm-run-all build-css build-js",
        "lint": "eslint src",
        "test-unit": "react-scripts test --env=jsdom --watchAll",
        "test-unit-ci": "react-scripts test --env=jsdom --no-watch",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "babel-eslint": "10.1.0",
        "babel-jest": "^24.9.0",
        "eslint-config-synacor": "3.0.5",
        "eslint-plugin-flowtype": "5.1.3",
        "eslint-plugin-import": "2.21.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.20.0",
        "node-sass-chokidar": "1.4.0",
        "npm-run-all": "4.1.5",
        "react-scripts": "3.4.1",
        "why-did-you-update": "1.0.8"
    },
    "eslintIgnore": [
        "build/*",
        "registerServiceWorker.js"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ]
}
